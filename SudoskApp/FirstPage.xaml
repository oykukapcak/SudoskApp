<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:SudoskApp" x:Class="SudoskApp.FirstPage">
	<StackLayout>
		<Image></Image>
		<Entry Placeholder="E-mail"></Entry>
		<Entry Placeholder="Password"></Entry>
		<Label Text="Forgot password?"></Label>
		<Button Text="Log in" Clicked="onLoginClicked"></Button>
		<Button Text="Create account" Clicked="onSignupClicked"></Button>
	</StackLayout>
</ContentPage>
