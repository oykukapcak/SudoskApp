<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="SudoskApp.MyItems">
	<ContentPage.Content>
	<ListView x:Name="list">
	    <ListView.ItemTemplate>
	      <DataTemplate>
	        <ViewCell>
	          <StackLayout Orientation="Horizontal">
	            <Image></Image>
	            <StackLayout Orientation="Vertical">
					<Label Text="{Binding Path=Name}"></Label>
						<Label Text="{Binding Path=BeginDate}"></Label>
						<Label Text="{Binding Path=EndDate}"></Label>
						<Label Text="{Binding Path=EventId}"></Label>		
						<Label Text="{Binding Path=Status}"></Label>
<!--	              <Label Text="Name"></Label>
	              <Label Text="Dates"></Label>
	              <Label Text="Event"></Label>
	              <Label Text="Status"></Label>-->
	            </StackLayout>
	          </StackLayout>
	        </ViewCell>
	      </DataTemplate>
	    </ListView.ItemTemplate>
	  </ListView>
	</ContentPage.Content>
</ContentPage>
